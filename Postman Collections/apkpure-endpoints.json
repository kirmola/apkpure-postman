							"// Fetch the Pako library\r",
							"pm.sendRequest(\"https://cdnjs.cloudflare.com/ajax/libs/pako/2.1.0/pako.min.js\", (err, res) => {\r",
							"    if (err) {\r",
							"        console.error(\"Error fetching Pako library:\", err);\r",
							"        return;\r",
							"    }\r",
							"\r",
							"    // Load the Pako library\r",
							"    eval(res.text());\r",
							"\r",
							"    // Get the raw JSON payload from the Body tab\r",
							"    const payload = pm.request.body.raw;\r",
							"\r",
							"    // Compress the payload with Pako (Gzip)\r",
							"    const compressedPayload = pako.gzip(payload);\r",
							"\r",
							"    // Convert the compressed payload to a Base64 string\r",
							"    const compressedBase64 = btoa(String.fromCharCode.apply(null, compressedPayload));\r",
							"\r",
							"    // Update the request body with the compressed payload\r",
							"    pm.request.body.update(Buffer.from(atob(compressedBase64)));\r",
							"\r",
							"    // Ensure the proper Content-Encoding header is set\r",
							"    pm.request.headers.add({\r",
							"        key: \"Content-Encoding\",\r",
							"        value: \"gzip\"\r",
							"    });\r",
							"\r",
							"    console.log(\"Payload successfully compressed and updated!\");\r",
							"});\r",
							""
						],
